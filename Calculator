print("Calculator")
num1 = int(input("Enter first number: "))
num2 = int(input("Enter second number: "))
operation = input("Select operation (+, -, *, /): ")

if operation == '+':
    print(num1 + num2)

elif operation == '-':
    print(num1 - num2)

elif operation == '*':
    print(num1 * num2)

elif operation == '/':
    if num2 == 0:
        print("Error: cannot divide by zero")
    else:
        print(num1 / num2)

else:
    print("Invalid operation selected")
